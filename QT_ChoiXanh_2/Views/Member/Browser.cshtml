                src: formContent,
                type: 'html',
                afterShow: function (instance, current) {
                    var $form = $(instance.$content).find('#formdulieu');
                    $form.on('submit', function (e) {
                        e.preventDefault();
                        var formData = $form.serialize();
                        $.ajax({
                            url: `add.asp?act=&gdc=&id=${id}&id1=${$this.hasClass('doi-pass') ? 'matkhau' : ''}`,
                            type: 'post',
                            data: formData,
                            async: true,
                            success: function (responseText) {
                                $(".formthongbaopopupcenter").html(responseText);
                                if (window.parent) {
                                    window.parent.$(window.parent.document).trigger("reload_table");
                                }
                                $.fancybox.close();
                            },
                            error: function (xhr, status, error) {
                                console.error('Lỗi AJAX:', error);
                            }
                        });
                    });
                }
            });
        });

