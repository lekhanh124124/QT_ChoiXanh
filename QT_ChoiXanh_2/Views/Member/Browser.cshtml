                    return `${statusIcon} <span class="hoten">${ten && ten.trim() !== '' ? ten : user}</span>`;
                },
            },
            { data: 'email' },
            { data: 'diachi' },
            { data: 'tel' },
            { data: 'thoigian' },
            {
                data: "id",
                render: (data, type, row) => {
                    let id = row.id;
                    let LoaiKhachHang = row.loai;
