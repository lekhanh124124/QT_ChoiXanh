                                        <div class="col-sm-10">
                                            <input class="form-control" name="CustomerUsername" maxlength="20" value="${rowData.ten || rowData.user || ''}" readonly>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">Email:</label>
                                        <div class="col-sm-10">
                                            <input class="form-control" name="EmailAddress" maxlength="50" value="${rowData.email || ''}" readonly>
                                        </div>
                                    </div>
                                </div>
                                <div class="box-footer">
                                    <button type="submit" class="btn pull-right btn-primary nutdangnhap" name="Submit">Xác nhận xóa</button>
                                    <a class="btn btn-default" data-fancybox-close>Đóng</a>
                                </div>
                            </div>
                        </form>
                    </section>
