<script>
    $(document).ready(function () {
        var columns = [
            {
                data: 'ten',
                render: (data, type, row) => {
                    let id = row.id;
                    let ten = row.ten;
                    let user = row.user;
                    let trangthai = row.status;
                    let statusIcon = trangthai == "True"
                        ? `<a href="javascript:void(0)" class="hidethis" data-id="${id}" title="INACTIVE danh mục"><i class="tdicon fa fa-check-square"></i></a>`
